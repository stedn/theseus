	Journal = {Journal of Non-Newtonian Fluid Mechanics},
	Number = {3},
	Pages = {153 - 190},
	Title = {Viscoelastic flow of polymer solutions around a periodic, linear array of cylinders: comparisons of predictions for microstructure and flow fields},
