	Isbn = {0006-3495; 1542-0086},
	J1 = {Biophys J},
	Journal = {Biophysical Journal},
	Month = {07},
	Number = {1},
	Pages = {151--160},
	Title = {Apparent viscosity and cortical tension of blood granulocytes determined by micropipet aspiration.},
	Ty = {JOUR},
