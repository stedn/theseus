	Issn = {0022-2836},
	Journal = {Journal of Molecular Biology},
	Keywords = {rheology},
	Number = {4},
	Pages = {637 - 644},
	Title = {Divalent Cations Crosslink Vimentin Intermediate Filament Tail Domains to Regulate Network Mechanics},
