	Eprint = {http://dx.doi.org/10.4161/cib.18286},
	Journal = {Communicative \& Integrative Biology},
	Number = {1},
	Pages = {39-42},
	Title = {Active actin gels},
	Url = {http://dx.doi.org/10.4161/cib.18286},
	Volume = {5},
