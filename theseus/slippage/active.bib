	Journal = {The Journal of Cell Biology},
	Number = {6},
	Pages = {1343-1355},
	Title = {Cortical and cytoplasmic flow polarity in early embryonic cells of Caenorhabditis elegans.},
	Url = {http://jcb.rupress.org/content/121/6/1343.abstract},
	Volume = {121},
	Year = {1993},
	Bdsk-Url-1 = {http://jcb.rupress.org/content/121/6/1343.abstract},
