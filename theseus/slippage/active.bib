	Journal = {Proceedings of the National Academy of Sciences},
	Number = {51},
	Pages = {20820-20825},
	Title = {F-actin buckling coordinates contractility and severing in a biomimetic actomyosin cortex},
	Url = {http://www.pnas.org/content/109/51/20820.abstract},
	Volume = {109},
	Year = {2012},
	Bdsk-Url-1 = {http://www.pnas.org/content/109/51/20820.abstract},
